{
  "version": 2,
  "name": "datanext",
  "builds": [{
    "src": "package.json",
    "use": "@now/next"
  }],
  "routes": [{
      "src": "/object/(.+)",
      "dest": "/object?object=$1"
    },
    {
      "src": "/package/([^\/]+)/([^\/]+)",
      "dest": "/package?package=$1&metadata=$2"
    }
  ]
}
